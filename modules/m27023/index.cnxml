      <para id="id1167257589926">Lees.   </para>
      <figure id="id7395741">
        <media id="id7395741_media" alt="">
          <image mime-type="image/png" src="../../media/graphics4-e60b.png" id="id7395741__onlineimage" height="93" width="175"/>
